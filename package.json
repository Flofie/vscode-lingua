{
    "name": "lingua",
    "displayName": "lingua",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.39.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "lingua.analyse",
                "title": "Lingua - Analyse Ionic i18n string literals"
            },
            {
                "command": "lingua.gotoTranslation",
                "title": "Lingua - Goto translation"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "lingua.gotoTranslation",
                    "group": "navigation@1.31"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.39.0",
        "glob": "^7.1.4",
        "mocha": "^6.1.4",
        "typescript": "^3.3.1",
        "tslint": "^5.12.1",
        "vscode-test": "^1.2.0"
    },
    "dependencies": {
        "@types/lodash": "^4.14.144",
        "json-source-map": "^0.6.1",
        "lodash": "^4.17.15"
    }
}
